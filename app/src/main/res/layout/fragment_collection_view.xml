<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/collection_rea"
    android:layout_width="match_parent"
    android:layout_height="140dp"
    android:layout_marginBottom="4dp"
    android:layout_marginLeft="8dp"
    android:layout_marginRight="8dp"
    android:layout_marginTop="4dp"
    android:background="@drawable/b01">


    <TextView
        android:id="@+id/collection_textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="12dp"
        android:layout_marginTop="10dp"
        android:text="广东省珠海市"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/collection_textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="12dp"
        android:layout_marginStart="12dp"
        android:text="30"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:textSize="50dp" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/collection_textView2"
        android:layout_toEndOf="@+id/collection_textView2"
        android:layout_toRightOf="@+id/collection_textView2"
        android:text="°C"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/collection_textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/collection_textView2"
        android:layout_alignStart="@+id/collection_textView2"
        android:layout_below="@+id/collection_textView2"
        android:text="多云"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="17dp" />

    <TextView
        android:id="@+id/collection_textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/textView4"
        android:layout_centerHorizontal="true"
        android:text="35°"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/collection_textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/collection_textView4"
        android:layout_alignStart="@+id/collection_textView4"
        android:layout_below="@+id/collection_textView6"
        android:text="25°"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/collection_textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/collection_textView4"
        android:layout_marginLeft="53dp"
        android:layout_marginStart="53dp"
        android:layout_toEndOf="@+id/collection_textView4"
        android:layout_toRightOf="@+id/collection_textView4"
        android:text="东北风"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/collection_textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/collection_textView6"
        android:layout_alignStart="@+id/collection_textView6"
        android:layout_alignTop="@+id/collection_textView5"
        android:text="0%"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="18dp" />

    <TextView
        android:id="@+id/collection_textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/collection_textView6"
        android:layout_toEndOf="@+id/collection_textView6"
        android:layout_toRightOf="@+id/collection_textView6"
        android:text="5级"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:textSize="18dp" />

    <View
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:duplicateParentState="true"
        android:background="@drawable/bodercollectionout"
        />

</RelativeLayout>


